package com.excelr.assignment.java;


public class Variable {
	
	String name="Rachana";
	int num=8;
	float busfee=5000.99f;
	double collegefee=25000.56677423233;
	long phoneNo=9456789043l;
	char letter='R';
	boolean isgood= true;
	byte age=21;
	short distance=330;
	
	public void main(String[] args) {
		System.out.println("Running main in Var class");
		System.out.println("Name of the Student: " + name);
		System.out.println("Number of the Student: " + num);
		System.out.println("Bus Fees: "+busfee);
		System.out.println("College Fees: "+collegefee);
		System.out.println("Phone Number: "+phoneNo);
		System.out.println("Letter: "+letter);
		System.out.println("Is She is a Good Girl: "+isgood);
		System.out.println("age: "+age);
		System.out.println("distance: "+distance);
		
	}
	
}
	
	